{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "Capability for the main window",
	"windows": ["main"],
	"permissions": [
		"path:default",
		"event:default",
		"window:default",
		"app:default",
		"image:default",
		"resources:default",
		"menu:default",
		"tray:default",
		"webview:default",
		"webview:allow-internal-toggle-devtools",
		"os:allow-os-type",
		"window:allow-start-dragging",
		"dialog:allow-open"
	]
}
